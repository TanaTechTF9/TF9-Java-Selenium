	"item": [
		{
			"name": "Get a Random Dog Image",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"const res = pm.response.json()",
							"",
							"pm.test(\"Response contains image url\", function () {",
							"    pm.expect(res[\"status\"]).to.be.eq(\"success\")",
							"})",
							""
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "GET",
				"header": [],
				"url": "{{baseURL}}/breeds/image/random"
			},
			"response": []
		},
		{
			"name": "Get a Random Dog Image  with wrong parameters",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 404\", function () {",
							"    pm.response.to.have.status(404)",
							"});",
							"",
							"const res = pm.response.json()",
							"",
							"pm.test(\"Error messge is displayed in Response\", function (){",
							"    pm.expect(res[\"message\"]).to.contain(\"No route found for\")",
							"})",
							""
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "GET",
				"header": [],
				"url": "{{baseURL}}/breeds/image/rando"
			},
			"response": []
		}
	],
	"variable": [
		{
			"key": "baseURL",
			"value": "https://dog.ceo/api"
		}
	]
